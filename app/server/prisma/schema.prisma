generator client {
  provider = "prisma-client-js"
  output   = "../src/__generated__/prisma-client"
}

datasource db {
  provider = "postgresql"
  url      = env("PG_URL")
}

model User {
  id    Int      @id @default(autoincrement())
  email String   @unique
  role  UserRole
}

enum UserRole {
  HR
  CANDIDATE
}
